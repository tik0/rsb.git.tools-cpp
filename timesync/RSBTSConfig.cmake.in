IF(NOT RSBTS_CONFIG_PROCESSED)
    SET(RSBTS_CONFIG_PROCESSED TRUE)

    GET_FILENAME_COMPONENT(RSBTS_CONFIG_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

    SET(RSBTS_INCLUDE_DIRS "${RSBTS_CONFIG_DIR}/../../include")
    SET(RSBTS_RUNTIME_LIBRARY_DIRS "${RSBTS_CONFIG_DIR}/../../bin")

    INCLUDE("${RSBTS_CONFIG_DIR}/RSBTSDepends.cmake")
    SET(RSBTS_LIBRARIES @LIBRARY_NAME@)
        
ENDIF()
